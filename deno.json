{
  "ignore": ["tslib/*"],
  "tasks": {
    "transpile-tslib": "deno run -A ./bin/transpile_tslib.ts > globals.json",
    "test": "deno test --parallel"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.4",
    "@std/testing": "jsr:@std/testing@^1.0.2",
    "ts-morph": "npm:ts-morph@^24.0.0",
    "typescript": "npm:typescript@^5.5.3"
  },
  "fmt": {
    "lineWidth": 160,
    "exclude": ["./data/*"]
  },
  "lint": {
    "exclude": ["./data/*"]
  }
}
